<%- include("partials/header"); -%>

<h1>Home
  <span>
    <a class="add-button btn btn-primary btn-lg" href="/compose">Compose Journal</a>
  </span>
</h1>
<p><%= startingContent %></p>

<% posts.forEach(function(post){ %>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 100) + " . . .   " %>
    <a class="read-links" href="/post/<%= post.title %>">Read More</a>
  </p> %>
<% }); %>





<%- include("partials/footer"); -%>
